            "editorMode": "code",
            "expr": "100-((sum(increase(dsnet_core_accesser_http_errors_root_cause{status =~ \"500|503\",root_cause !~ \"delegated_index_batch|multidelete|load_shed\"}[$__interval]))/sum(increase(dsnet_core_accesser_http_requests[10m])))*100)",
